{% import 'macros/macros.html.twig' as nav_macros %}

<ul>
  {{ nav_macros.nav_loop(pages) }}

  {% if theme_var('custommenus.enabled') %}
    {% for mitem in theme_var('custommenu') %}
      <li>
        <a href="{{ mitem.url }}" target="{{ mitem.target }}">
          {% if mitem.icon %}
            <i class="fa fa-{{ mitem.icon }}"></i>
          {% endif %}
          {{ mitem.text|raw }}
        </a>
      </li>
    {% endfor %}
  {% endif %}
</ul>
